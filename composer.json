{
    "name": "dptsi/laravel-sso",
    "description": "Laravel login package for SSO ITS",
    "type": "library",

    "authors": [
      {
        "name": "DPTSI ITS",
        "email": "developer@its.ac.id"
      }
    ],
    "minimum-stability": "dev",
    "require": {
      "php": "^7.4|8.0.*",
      "laravel/framework": "^8.12",
      "myits/openid-connect-client": "^1.4.0",
      "ramsey/uuid": "^4.0.0"
    },
    "autoload": {
      "psr-4": {
        "Dptsi\\Sso\\": "src/"
      },
      "files": [
        "src/helpers.php"
      ]
    },
    "extra": {
      "laravel": {
        "providers": [
          "Dptsi\\Sso\\Providers\\SsoServiceProvider"
        ]
      }
    }
  }